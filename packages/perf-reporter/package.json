{
  "name": "@toolkit/perf-reporter",
  "version": "1.0.0",
  "description": "Lightweight Core Web Vitals collection and reporting",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["performance", "web-vitals", "metrics", "LCP", "CLS", "INP", "typescript"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.15.0"
  }
}
